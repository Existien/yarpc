{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Schema for python output",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "name",
        "language",
        "location"
    ],
    "properties": {
        "name": {"type": "string"},
        "language": {"const": "py"},
        "location": {"type": "string"},
        "services": {
            "type": "array",
            "items": {"$ref": "common.schema.json#/interface"}
        },
        "clients": {
            "type": "array",
            "items": {"$ref": "common.schema.json#/interface"}
        },
        "service_mocks": {
            "type": "array",
            "items": {"$ref": "common.schema.json#/interface"}
        },
        "client_mocks": {
            "type": "array",
            "items": {"$ref": "common.schema.json#/interface"}
        }
    }
}