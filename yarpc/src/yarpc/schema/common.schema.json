{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Reusable schemata",
    "camelName": {
        "type": "string",
        "pattern": "^[a-z][a-zA-Z0-9]*$"
    },
    "pascalName": {
        "type": "string",
        "pattern": "^[A-Z][a-zA-Z0-9]*$"
    },     
    "docs": {
        "type": "string"
    },
    "type": {
        "type": "string",
        "pattern": "^[a-zA-Z][a-zA-Z0-9<,>]*$"
    }
}